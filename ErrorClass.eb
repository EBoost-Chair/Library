@!Define Error Class
public class Error(Object)
 private sub &build
  super
  if ARGU[0].classof==String
   public Msg=ARGU[0]
  else
   public Msg="Unknown Error"
  end if
 end sub
end class

public class Warning(Object)
 private sub &build
  super
  if ARGU[0].classof==String
   public Msg=ARGU[0]
  else
   Msg="Unknown Warning"
  end if
 end sub
end class